{
	"name": "@kamado-io/page-compiler",
	"version": "2.0.0-alpha.1",
	"description": "Page compiler for Kamado",
	"author": "D-ZERO Co., Ltd.",
	"license": "MIT",
	"publishConfig": {
		"access": "public"
	},
	"type": "module",
	"main": "./dist/page-compiler.js",
	"types": "./dist/page-compiler.d.ts",
	"exports": {
		".": {
			"types": "./dist/page-compiler.d.ts",
			"default": "./dist/page-compiler.js"
		},
		"./page-transform": {
			"types": "./dist/page-transform.d.ts",
			"default": "./dist/page-transform.js"
		},
		"./transform/manipulate-dom": {
			"types": "./dist/transform/manipulate-dom.d.ts",
			"default": "./dist/transform/manipulate-dom.js"
		},
		"./transform/character-entities": {
			"types": "./dist/transform/character-entities.d.ts",
			"default": "./dist/transform/character-entities.js"
		},
		"./transform/doctype": {
			"types": "./dist/transform/doctype.d.ts",
			"default": "./dist/transform/doctype.js"
		},
		"./transform/prettier": {
			"types": "./dist/transform/prettier.d.ts",
			"default": "./dist/transform/prettier.js"
		},
		"./transform/minifier": {
			"types": "./dist/transform/minifier.d.ts",
			"default": "./dist/transform/minifier.js"
		},
		"./transform/line-break": {
			"types": "./dist/transform/line-break.d.ts",
			"default": "./dist/transform/line-break.js"
		},
		"./transform/inject-to-head": {
			"types": "./dist/transform/inject-to-head.d.ts",
			"default": "./dist/transform/inject-to-head.js"
		},
		"./transform/ssi-shim": {
			"types": "./dist/transform/ssi-shim.d.ts",
			"default": "./dist/transform/ssi-shim.js"
		},
		"./dev-transform/inject-to-head": {
			"types": "./dist/transform/inject-to-head.d.ts",
			"default": "./dist/transform/inject-to-head.js"
		},
		"./dev-transform/ssi-shim": {
			"types": "./dist/transform/ssi-shim.d.ts",
			"default": "./dist/transform/ssi-shim.js"
		},
		"./title": {
			"types": "./dist/features/title-utils.d.ts",
			"default": "./dist/features/title-utils.js"
		}
	},
	"scripts": {
		"build": "tsc --project tsconfig.build.json",
		"dev": "tsc --watch --project tsconfig.build.json"
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"@d-zero/shared": "0.20.1",
		"ansi-colors": "4.1.3",
		"character-entities": "2.0.2",
		"fast-glob": "3.3.3",
		"html-minifier-terser": "7.2.0",
		"image-size": "2.0.2",
		"kamado": "2.0.0-alpha.1",
		"prettier": "3.8.1"
	},
	"devDependencies": {
		"@types/html-minifier-terser": "7.0.2",
		"typescript": "5.9.3"
	}
}
